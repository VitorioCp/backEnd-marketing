generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:../src/db/database.db"
}

model users {
  id       Int    @id @default(autoincrement())
  email    String @unique(map: "sqlite_autoindex_users_1")
  login    String @unique(map: "sqlite_autoindex_users_2")
  password String
}
